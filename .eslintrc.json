{
    "root": true,
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
	"plugin:@typescript-eslint/eslint-recommended",
        "plugin:@typescript-eslint/recommended",
	"prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
        "@typescript-eslint",
	"prettier"
    ],
    "rules": {
	"prettier/prettier": 2
    }
}
